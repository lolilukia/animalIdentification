Page{
  background: #efefef;
}
.listContain{
  padding: 30rpx;
  background: #fff;
}
.listTitle{
  height: 80rpx;
  width: 80%;
  margin: 0 auto;
  border-bottom: 1px solid #eee;
}
.listContent{
  text-align: left;
  color: #666;
  word-break: break-all;
  background: #fff;
}
.listBlue{
  color: #3498db;
  word-break: break-all;
}
.flex-wrp{
  display: flex;
  flex-direction:row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.imgContain{
  width: 80%;
  margin: 0 auto;
  padding-top: 10rpx;
}
.moduleCon{
  position: relative;
}
.imgPanel{
  width: 200rpx;
  height: 200rpx;
  border-radius: 1000rpx;
  overflow: hidden;
  margin: 0 20rpx 20rpx 0;
  position: relative;
}
.listImg{
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.tag{
  width: 200rpx;
  height: 40rpx;
  background: #fff;
  color: #3498db;
  font-weight: 600;
  text-align: center;
  font-size: 80%;
  position: absolute;
  left: 0;
  bottom: 10rpx;
}
